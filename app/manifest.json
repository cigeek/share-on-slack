{
  "name": "Share on Slack",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Share current page as a post on Slack",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Share on Slack",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
